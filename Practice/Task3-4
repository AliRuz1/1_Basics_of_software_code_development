public class Multi200 {

    /*
     Составить программу нахождения произведения квадратов первых двухсот чисел
     */

    public static void main(String[] args){
        double fact = 1;
        double i;
        for (i = 1; i< 200; i++){
            fact = fact *Math.pow(i,2);
        }
        System.out.println(fact);

    }   
}
